{
  "name": "ten_meter_walk_test",
  "version": "1.0.0",
  "description": "Benchmark for measuring a patient's rehabilitation progress overtime.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node src/app.js",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tellisense/ten_meter.git"
  },
  "author": "Justin Rahimi  tellisense@icoud.com  @Tellisense",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/Tellisense/ten_meter/issues"
  },
  "homepage": "https://github.com/Tellisense/ten_meter#readme",
  "keywords": [
    "10",
    "meter",
    "walk",
    "test",
    "physical",
    "therapy",
    "Orthotist",
    "prosthetist",
    "orthotics",
    "fitter"
  ]
}
